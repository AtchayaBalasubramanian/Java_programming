import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        int n1=sc.nextInt();
        float n2=sc.nextFloat();
        System.out.println(n1);
        System.out.printf("%.2f",n2);
    }
}
