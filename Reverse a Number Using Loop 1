import java.io.*;
import java.util.*;
public class Demo{
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
        int rev=0,rem=0,num=n;
        if(num>0 && num<1000000000 )
        {
            while(num>0){
            rem=num%10;
            rev=rev*10+rem;
            num=num/10;}
            System.out.println(rev);
        }
        else
        {
            System.out.println("Invalid Input ");
        }
    }
}
