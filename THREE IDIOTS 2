import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        int x1=sc.nextInt();
        int y1=sc.nextInt();
        int x2=sc.nextInt();
        int y2=sc.nextInt();
        float midx=(x1+x2)/2f;
        float midy=(y1+y2)/2f;
        System.out.println("Binoy's house is located at ("+midx+","+midy+")");
    }
}
