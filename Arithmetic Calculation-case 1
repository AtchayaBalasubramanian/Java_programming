import java.io.*;
import java.util.*;
public class Solution {
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        int a=sc.nextInt(),b=sc.nextInt();
        char ch=sc.next().charAt(0);
        double result=0.0;
        switch(ch)
        {
            case '+':
                result=a+b;
                System.out.println("Addition of two number is "+result);
                break;
            case '-':
                result=a-b;
                System.out.println("Subtraction of two number is "+result);
                break;
            case '*':
                result=a*b;
                System.out.println("Multiplication of two number is "+result);
                break;
            case '/':
                result=a/b;
                System.out.println("Division of two number is "+result);
                break;
            case '%':
                result=a%b;
                System.out.println("Modulo of two number is "+result);
                break;
            case '^':
                result=a^b;
                System.out.println("Square of two number is "+result);
                break;
            default:
                System.out.println("Invalid Input");
        }
        
    }
}
