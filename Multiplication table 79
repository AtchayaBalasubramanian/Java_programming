import java.io.*;
import java.util.*;
public class Solution {
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
            if(n>=1 && n<=9){
            for(int i=1;i<=n;i++)
            {
            int prod=n*i;
            System.out.println(n+" x "+i+" = "+prod);
            }
            }
            else
            {
              System.out.println("Invalid Input ");  
            }
        
        
    }
}
